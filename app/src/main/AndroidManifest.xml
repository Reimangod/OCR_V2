<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.ocr_v2">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ocr_v2"
        tools:targetApi="31">
        <activity
            android:name=".HomeScreen"
            android:exported="false"
            android:label="@string/title_activity_home_screen"
            android:theme="@style/Theme.Ocr_v2" />
        <activity
            android:name=".MainScreen"
            android:exported="false"
            android:label="@string/title_activity_main_screen"
            android:theme="@style/Theme.Ocr_v2" />
        <activity
            android:name=".TextFieldList"
            android:exported="false"
            android:label="@string/title_activity_text_field_list"
            android:theme="@style/Theme.Ocr_v2" />
        <activity
            android:name=".TextFieldItem"
            android:exported="false"
            android:label="@string/title_activity_text_field_item"
            android:theme="@style/Theme.Ocr_v2" />
        <activity
            android:name=".TextField"
            android:exported="false"
            android:label="@string/title_activity_text_field"
            android:theme="@style/Theme.Ocr_v2" />
        <activity
            android:name=".LineList"
            android:exported="false"
            android:label="@string/title_activity_line_list"
            android:theme="@style/Theme.Ocr_v2" />
        <activity
            android:name=".LineItem"
            android:exported="false"
            android:label="@string/title_activity_line_item"
            android:theme="@style/Theme.Ocr_v2" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Ocr_v2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>